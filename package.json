{
	"name": "cleana",
	"type": "module",
	"version": "2.1.0",
	"description": "Clean JavaScript Objects and Arrays, recursively. Lightweight and Fast.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Saeid-Za/cleana.git"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs"
		}
	},
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "obuild ./src/index.ts",
		"lint": "eslint . --fix",
		"typecheck": "tsc --noEmit",
		"test": "vitest run --coverage",
		"dev": "vitest dev",
		"benchmark": "npm run build && jiti benchmark/index.ts",
		"benchmark:all": "npm run build && jiti benchmark/index.ts --only-cleana-fastclean",
		"prepack": "npm run build",
		"release": "npm run typecheck && npm run lint && npm run test && npm publish && git push --follow-tags"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^7.2.0",
		"@types/node": "^25.2.0",
		"@vitest/coverage-v8": "^4.0.18",
		"automd": "^0.4.3",
		"changelogen": "^0.6.2",
		"clean-deep": "^3.4.0",
		"deep-cleaner": "^2.0.0-alpha.3",
		"eslint": "^9.39.2",
		"fast-clean": "^1.5.3",
		"jiti": "^2.6.1",
		"mitata": "^1.0.34",
		"obj-clean": "^3.0.1",
		"obuild": "^0.4.22",
		"typescript": "^5.9.3",
		"vitest": "^4.0.18"
	}
}
